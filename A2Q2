//Setup Function
function setup() {
  createCanvas(400, 400);
}
//Draw Function
function draw() {
  background(220, 100, 130);  //Set background to red
  
  //Create the Shape
  fill("lightblue")
  beginShape();     //begin Shape
  vertex(50, 40);
  vertex(350, 40);
  vertex(350, 140);
  vertex(300, 120);
  vertex(300, 300);
  vertex(150, 350);
  vertex(150, 100);
  vertex(80, 120);
  vertex(60, 60);
  vertex(50, 90);
  endShape();       //End shape
}
